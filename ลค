def cms(string, commands): #/XXX, >XXX, ;XXX, ^XXX, %XXX, $XXX...
    tex = ["+","@","/",">",";","^","%","$","Ôºæ","„Çµ„ÉÜ„É©:","„Çµ„ÉÜ„É©:","„Çµ„ÉÜ„É©Ôºö","„Çµ„ÉÜ„É©Ôºö"]
    for texX in tex:
        for command in commands:
            if string ==command:
                return True
    return False

